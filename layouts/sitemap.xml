{{- printf "<?xml version=\"1.0\" encoding=\"utf-8\" standalone=\"yes\" ?>" | safeHTML }}<urlset xmlns="https://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xhtml="https://www.w3.org/1999/xhtml">
  {{ range .Data.Pages }}
	  {{ if not .Params.noindex }}
	  <url>
	    <loc>{{ .Permalink }}</loc>
	    <lastmod>{{ safeHTML ( .Date.Format "2006-01-02T15:04:05-07:00" ) }}</lastmod>{{ with .Sitemap.ChangeFreq }}
	    <changefreq>{{ . }}</changefreq>{{ end }}{{ if ge .Sitemap.Priority 0.0 }}
	    <priority>{{ .Sitemap.Priority }}</priority>{{ end }}
	  </url>
	  {{ end }}
  {{ end }}
</urlset>